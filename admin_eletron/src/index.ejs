<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>power-grid-admin</title>
    <% if (htmlWebpackPlugin.options.nodeModules) { %>
      <!-- Add `node_modules/` to global paths so `require` works properly in development -->
      <script>
        require('module').globalPaths.push('<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>')
      </script>
    <% } %>
    <!--script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/jquery.min.js></script-->
    <script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/BinaryHeap.js></script>
		<script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/stats.min.js></script>
		<script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/tween.min.js></script>
		<script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/proj4.js></script>
		<script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/laslaz.js></script>
    <script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/three.min.js></script>
    <!--script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/jquery-ui/jquery-ui.min.js></script-->
    <script src=<%= htmlWebpackPlugin.options.path %>/static/potree/potree.js></script>
    <script src=<%= htmlWebpackPlugin.options.path %>/static/cesium/cesium.js></script>
    <script src=<%= htmlWebpackPlugin.options.path %>/static/potree/lib/i18next.js></script>

  </head>
  <body>
    <div id="app"></div>
    <!-- Set `__static` path to static files in production -->
    <script>
      if (process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '/static').replace(/\\/g, '\\\\')
    </script>

    <!-- webpack builds are automatically injected -->
  </body>
</html>
